{
    "name":"Corset",
    "version":"1.0",
    "description":"Allows requests to bypass the cross-origin-request security",
    "permissions":["webRequest", "webRequestBlocking",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background":{
        "scripts":["corset.js"]
    },
    "options_page":"options.html",
    "manifest_version":2
}
